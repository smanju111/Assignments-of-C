#include <stdio.h>

int main()
{
  int n1, n2, i;
  printf("Enter the starting number: ");
  scanf("%d", &n1);
  printf("\nEnter the ending number: ");
  scanf("%d", &n2);
  printf("\nOdd numbers between %d and %d are: \n", n1, n2);
  for(i=n1; i<=n2; i++)
  {
    if(i%2 != 0)
    {
      printf("%d\t", i);
    }
  }
  return 0;
}
